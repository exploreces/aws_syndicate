{
  "audit_producer-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution",
      {
        "policy_name": "dynamodb-stream-access",
        "policy_content": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:GetShardIterator",
                "dynamodb:GetRecords",
                "dynamodb:DescribeStream",
                "dynamodb:ListStreams",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:GetItem",
                "dynamodb:PutItem"
              ],
              "Resource": "*"
            }
          ]
        }
      }
    ],
    "resource_type": "iam_role",
    "tags": {}
  }
}